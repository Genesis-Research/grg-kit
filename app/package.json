{
  "name": "app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "codegen": "node scripts/generate-sources.js",
    "codegen:blocks": "node scripts/generate-sources.js blocks",
    "codegen:components": "node scripts/generate-sources.js components",
    "codegen:grg": "node scripts/generate-sources.js grgComponents",
    "prebuild": "npm run codegen"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.0.0",
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ng-icons/core": ">=32.0.0 <34.0.0",
    "@ng-icons/lucide": ">=32.0.0 <34.0.0",
    "@ng-icons/remixicon": "^33.0.0",
    "@spartan-ng/brain": "0.0.1-alpha.572",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/angular-table": "^8.21.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "embla-carousel-angular": "^19.0.0",
    "embla-carousel-autoplay": "^8.6.0",
    "luxon": "^3.7.2",
    "ngx-sonner": ">=3.0.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "shiki": "^3.19.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.17",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.0",
    "@spartan-ng/cli": "0.0.1-alpha.572",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "~5.9.2"
  }
}
